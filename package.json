{
  "name": "@nicholasbrink/oauth-server-example",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "node app.js",
    "build": "npm run build-ts",
    "watch-node": "nodemon --inspect dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "express": "^4.16.2",
    "inversify": "^4.11.1",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.1.13",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/express": "^4.11.1",
    "@types/node": "^9.4.6",
    "@types/winston": "^2.3.8",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.1",
    "typescript": "^2.7.2"
  }
}
